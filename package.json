{
  "name": "eslint-config-lsage",
  "version": "0.2.0",
  "description": "This package provides ls-age's ESLint configuration",
  "main": "out/index.js",
  "scripts": {
    "compile": "babel src -d out",
    "docs": "mkdir -p docs/api && echo \"*not available yet\" > docs/api/README.md",
    "lint": "eslint src",
    "prepublishOnly": "npm run -s compile && npm run -s docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukasHechenberger/eslint-config-lsage.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "ls-age"
  ],
  "author": "Lukas Hechenberger <admin@ls-age.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LukasHechenberger/eslint-config-lsage/issues"
  },
  "homepage": "https://github.com/LukasHechenberger/eslint-config-lsage#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^4.11.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "./out/index.js"
  },
  "dependencies": {
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsdoc": "^3.1.1"
  }
}
